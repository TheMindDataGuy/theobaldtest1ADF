{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "theobaldtest1ADF"
		},
		"Blob1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Blob1'"
		},
		"DB1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DB1'"
		},
		"Blob1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://theobaldtest1blob.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "theobaldtest1ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Blob1_accountKey": {
						"value": "[parameters('Blob1_accountKey')]"
					},
					"DB1_connectionString": {
						"value": "[parameters('DB1_connectionString')]"
					},
					"Blob1_properties_typeProperties_url": {
						"value": "[parameters('Blob1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}